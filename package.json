{
	"name": "svelte-simple-markdown",
	"description": "Simple and Extensible Markdown Parser for any framework.",
	"version": "1.0.3",
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"peerDependencies": {
		"svelte": "^3.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.5.10",
		"tslib": "^2.3.1",
		"typescript": "^4.6.4"
	},
	"homepage": "https://github.com/davecaruso/svelte-simple-markdown#readme",
	"repository": "github:davecaruso/svelte-simple-markdown",
	"bugs": {
		"url": "https://github.com/davecaruso/svelte-simple-markdown/issues",
		"email": "dave@davecode.net"
	},
	"type": "module"
}
